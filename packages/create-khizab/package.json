{
  "name": "create-khizab",
  "description": "Create Khizab apps with one command",
  "version": "0.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/khizab/khizab.git",
    "directory": "packages/create-khizab"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --project tsconfig.build.json --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "bun src/cli.ts",
    "test:build": "publint --strict",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "templates/**"
  ],
  "bin": {
    "khizab": "./dist/esm/cli.js"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "cross-spawn": "^7.0.3",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.7.2",
    "@types/prompts": "^2.4.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1"
  },
  "contributors": ["awkweb.eth <t@wevm.dev>", "jxom.eth <j@wevm.dev>"],
  "funding": "https://github.com/sponsors/wevm",
  "keywords": ["khizab", "eth", "ethereum", "dapps", "wallet", "web3", "cli"]
}
